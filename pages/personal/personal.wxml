<view class="head">
  <view class="my-hand">
    <text class="my">{{my}}</text>
  </view>
  <view class="information clear">
    <image class="img" src="{{img}}"></image>
    <view class="li">
      <text class="name">{{name}}</text>
      <text class="address">{{address}}</text>
    </view>
    <view class="my-share">
      <text class="{{icon}}"></text>
      <text class="share">{{share}}</text>
    </view>
  </view>
  <view class="number">
    <view class="follow">
      <text class="list">{{list}}</text>
      <text class="list-follow">{{follow}}</text>
    </view>
    <view class="follow">
      <text class="list">{{list1}}</text>
      <text class="fans">{{fans}}</text>
    </view>
    <view class="follow1">
      <text class="list">{{list2}}</text>
      <text class="like">{{like}}</text>
    </view>

  </view>
  <view class="classification">
    <text class="ranking">{{rank2}}</text>
    <view>
      <text class="arrow iconfont icon-arrow-right"></text>
    </view>
  </view>
  <view class="classification">
    <text class="ranking">{{rank3}}</text>
    <!-- <view class="news">{{news}}</view> -->
    <text class="news">{{news}}</text>
  </view>
  <view class="classification">
    <text class="ranking">{{rank4}}</text>
    <view>
      <text class="balance">{{balance}}</text>
      <text class="arrow iconfont icon-arrow-right"></text>
    </view>
  </view>
  <view class="classification">
    <text class="ranking">{{rank5}}</text>
    <view>
      <text class="arrow iconfont icon-arrow-right"></text>
    </view>
  </view>
<view class="tt">
<image class="t5" src="{{src}}"></image>
</view>
</view>