/**app.wxss**/
@import './style/iconfont.wxss';

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

.clear::after {
  content: '';
  display: block;
  height: 0rpx;
  line-height: 0rpx;
  clear: both;
  visibility: hidden;
}
